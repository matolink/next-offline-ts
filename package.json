{
  "name": "next-offline-ts",
  "version": "6.1.5",
  "keywords": [
    "react",
    "next.js",
    "offline",
    "service worker",
    "pre-cache",
    "networkFirst",
    "cacheFirst",
    "webpack",
    "workbox",
    "sw-helpers"
  ],
  "bugs": "https://github.com/meabed/next-offline-ts/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/meabed/next-offline-ts.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc -p .",
    "lint": "eslint \"src/**/*.ts\"",
    "prepare": "yarn husky install",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\" \"__tests__/**/*.{ts,tsx,js,jsx}\" \"examples/**/*.{ts,tsx,js,jsx}\"",
    "test": "NODE_ENV=test jest --runInBand --forceExit"
  },
  "lint-staged": {
    "src/**/*.{ts,json}": "prettier --write",
    "__tests__/**/*.ts|js|tsx|jsx": "prettier --write",
    "examples/**/*.ts|js|tsx|jsx": "prettier --write",
    "./index.ts": "prettier --write"
  },
  "dependencies": {
    "@types/workbox-webpack-plugin": "5.1.8",
    "copy-webpack-plugin": "~10.2.0",
    "workbox-webpack-plugin": "^6.4.2"
  },
  "devDependencies": {
    "@types/jest": "27.4.0",
    "@types/prettier": "2.4.3",
    "@types/rimraf": "3.0.2",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "lint-staged": "12.2.2",
    "next": "12.0.8",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.3",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5",
    "webpack": "5.66.0"
  },
  "peerDependencies": {},
  "authors": [
    "Mohamed Meabed"
  ],
  "tags": [
    "react",
    "next.js",
    "offline",
    "service worker",
    "pre-cache",
    "networkFirst",
    "cacheFirst",
    "webpack",
    "workbox",
    "sw-helpers"
  ]
}
